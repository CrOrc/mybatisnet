<root>
    <!-- root of docbook stylesheets and images -->
    <property name="docbook.styles.dir" value="docbook/styles" />

    <!-- xsltproc executable - assumed to be on the path -->
    <property name="xsltproc.program" value="xsltproc.exe" />

    <!-- fop executable - Since this is a batch file, the full path is required,
         even if the folder is on the path. 
      -->
    <property name="fop.program" value="C:/fop/fop.bat" />
</root>
